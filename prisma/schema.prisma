generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  user_id       Int    @id @default(autoincrement())
  username      String @unique @db.VarChar(30)
  email         String @unique @db.VarChar(60)
  user_password String @db.VarChar(60)
}
